<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.notifications.PaybackActivity">

    <ImageButton
        android:layout_width="51dp"
        android:layout_height="51dp"
        app:srcCompat="@drawable/abc_vector_test"
        android:id="@+id/backButton"
        android:background="@drawable/rounded_corners"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="24dp"
        android:layout_marginStart="24dp"/>

    <Button
        android:id="@+id/paidBack"
        android:layout_width="200dp"
        android:layout_height="49dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/rounded_corners"
        android:text="Jeg har betalt tilbake"
        android:textColor="#FFFFFF"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/skylder"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/title"
        android:layout_width="370dp"
        android:layout_height="59dp"
        android:layout_marginTop="89dp"
        android:gravity="center"
        android:text="Du skylder "
        android:textSize="40dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/typeRett"
        android:layout_width="345dp"
        android:layout_height="40dp"
        android:layout_marginTop="24dp"
        android:text="Type Rett: "
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title" />

    <TextView
        android:id="@+id/dato"
        android:layout_width="345dp"
        android:layout_height="40dp"
        android:layout_marginTop="10dp"
        android:text="Dato: "
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/typeRett" />

    <TextView
        android:id="@+id/sted"
        android:layout_width="345dp"
        android:layout_height="40dp"
        android:layout_marginTop="10dp"
        android:text="Sted: "
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dato" />

    <TextView
        android:id="@+id/delesPaa"
        android:layout_width="345dp"
        android:layout_height="40dp"
        android:layout_marginTop="10dp"
        android:text="PÃ¥meldte gjester ved deling: "
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sted" />

    <TextView
        android:id="@+id/skylder"
        android:layout_width="345dp"
        android:layout_height="40dp"
        android:layout_marginTop="10dp"
        android:text="Betal til: "
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/delesPaa" />

</androidx.constraintlayout.widget.ConstraintLayout>