<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.home.ProfileActivity">

    <ImageButton
            android:layout_width="51dp"
            android:layout_height="51dp" app:srcCompat="@drawable/abc_vector_test"
            android:id="@+id/backButtonProfile" android:background="@drawable/rounded_corners"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="24dp" android:layout_marginTop="24dp" android:layout_marginStart="24dp"/>
    <ImageView
            android:layout_width="128dp"
            android:layout_height="128dp" tools:srcCompat="@tools:sample/avatars" android:id="@+id/imageView"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="140dp" android:layout_marginStart="140dp" android:layout_marginEnd="140dp"
            android:layout_marginRight="140dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="88dp"
            app:layout_constraintHorizontal_bias="0.0"/>
    <TextView
            android:text="Kari Nordmann"
            android:layout_width="160dp"
            android:layout_height="32dp" android:id="@+id/textView2"
            android:textSize="24sp" android:layout_marginTop="19dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="125dp" android:layout_marginStart="125dp" android:layout_marginEnd="125dp"
            android:layout_marginRight="125dp" app:layout_constraintEnd_toEndOf="parent"
    />
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="116dp"
            android:layout_height="254dp"
            android:background="#EE1C8722" android:layout_marginTop="231dp"
            app:layout_constraintTop_toBottomOf="@+id/backButtonProfile" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/linearLayout" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="171dp" android:id="@+id/linearLayout2">
        <FrameLayout
                android:layout_width="113dp"
                android:layout_height="19dp">

        </FrameLayout>
        <ImageView
                android:layout_width="133dp"
                android:layout_height="43dp" app:srcCompat="@android:drawable/ic_menu_myplaces"
                android:id="@+id/imageView2"/>
        <FrameLayout
                android:layout_width="113dp"
                android:layout_height="19dp">

        </FrameLayout>
        <ImageView
                android:layout_width="126dp"
                android:layout_height="wrap_content" app:srcCompat="@android:drawable/ic_dialog_email"
                android:id="@+id/imageView3"/>
        <FrameLayout
                android:layout_width="113dp"
                android:layout_height="19dp">

        </FrameLayout>
        <ImageView
                android:layout_width="129dp"
                android:layout_height="41dp" app:srcCompat="@android:drawable/ic_lock_idle_lock"
                android:id="@+id/imageView4"/>
        <FrameLayout
                android:layout_width="113dp"
                android:layout_height="19dp">

        </FrameLayout>
        <ImageView
                android:layout_width="124dp"
                android:layout_height="wrap_content" app:srcCompat="@android:drawable/ic_dialog_info"
                android:id="@+id/imageView5"/>
    </LinearLayout>
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="299dp"
            android:layout_height="254dp"
            android:background="#EE1C8722" android:id="@+id/linearLayout"
            app:layout_constraintStart_toEndOf="@+id/linearLayout2" android:layout_marginTop="39dp"
            app:layout_constraintTop_toBottomOf="@+id/textView2" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="171dp">
        <FrameLayout
                android:layout_width="118dp"
                android:layout_height="27dp">

        </FrameLayout>
        <TextView
                android:text="Kongens gate 14"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/textView3" android:textColor="#FFFFFF"
                android:textSize="18sp"/>
        <FrameLayout
                android:layout_width="120dp"
                android:layout_height="33dp">

        </FrameLayout>
        <TextView
                android:text="kari.nordmann@gmail.com"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/textView4" android:textColor="#FFFFFF"
                android:textSize="18sp"/>
        <FrameLayout
                android:layout_width="121dp"
                android:layout_height="33dp">

        </FrameLayout>
        <TextView
                android:text="*********"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/textView5" android:textColor="#FFFFFF"
                android:textSize="18sp"/>
        <FrameLayout
                android:layout_width="121dp"
                android:layout_height="33dp">

        </FrameLayout>
        <TextView
                android:text="Ingen alleriger"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/textView6" android:textColor="#FFFFFF"
                android:textSize="18sp"/>
    </LinearLayout>
    <ImageButton
            android:layout_width="49dp"
            android:layout_height="50dp" app:srcCompat="@android:drawable/ic_menu_edit"
            android:id="@+id/imageButton"
            android:background="@drawable/rounded_corners"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="24dp" android:layout_marginEnd="24dp" android:layout_marginRight="24dp"
            app:layout_constraintEnd_toEndOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>